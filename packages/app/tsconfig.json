{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react" /* Specify JSX code generation: 'preserve', 'react-native', or 'react'. */,
    "baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
    "typeRoots": [
      "./typings",
      "../../node_modules/@types"
    ] /* List of folders to include type definitions from. */
  },
  "include": ["../"],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
